<main style="margin: 1em">
  <h3>My Accounts</h3>
  <%=JSON.stringify(something)%>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Organization</th>
        <th scope="col">password</th>
        <th scope="col">Edit</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
      <!-- <% for(let url in urls) { %> -->
      <tr>
        <td><%= something.Organization %></td>
        <td><%= something.password%></td>
        <td>
          <form method="POST" action="/urls/update/<%= something.password %>">
            <button class="btn btn-outline-primary" type="submit">
              Edit
            </button>
          </form>
        </td>
        <td>
          <form method="POST" action="/urls/delete/<%= url %>">
            <!-- <input type="password" name="information"> -->
            <button class="btn btn-outline-danger" type="submit">
              Delete
            </button>
          </form>
        </td>
      </tr>
      <% } %>
    </tbody>
  </table>
</main>
