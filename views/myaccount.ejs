<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />

    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, maximum-scale=1.0"
    />
    <title>Passwallet- myaccount</title>
    <link rel="stylesheet" href="/styles/header.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
    <link rel="stylesheet" href="/styles/main.css" type="text/css" />
    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  </head>
  <body>
    <%- include('partials/_loggedheader') %>
    <main>
      <h3>My Accounts</h3>
      <table class="table">
        <!-- <thead>
          <tr>
            <th scope="col">Organization</th>
            <th scope="col">password</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
          </tr>
        </thead> -->
       <%JSON.stringify(passwords) %>

        <table>
          <tr>
            <th>id</th>
            <th>username</th>
            <th>homepage/loginpage</th>
            <th>password</th>
          </tr>
          <% for (const logins of passwords) { %>
            <tr>
                <td><%= logins.id %></td>
                <td><%= logins.user_name_for_site_login %></td>
                <td><%= logins.url_for_login %></td>
                <td><%= logins.user_password_for_site_login %></td>
                <td>
                  <a href="home/editLogin/<%=logins.id%>">
                    <button class="btn btn-outline-primary" type="submit">
                      Edit
                    </button>
                  </a>
                </td>
                <td>
                  <form method="POST" action="home/deleteLogin/<%=logins.id%>">
                    <button class="btn btn-outline-danger" type="submit">
                      Delete
                    </button>
                  </form>
                </td>
            </tr>
          <% } %>
          </table>
        <tbody>
        </tbody>
      </table>
    </main>
  </body>
</html>
