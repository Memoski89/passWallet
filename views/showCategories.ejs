  <body>
    <%- include('partials/_loggedheader') %>
    <main class="categories">
      <h3>My Login Categories</h3>
      <form method="GET" action="/home/:category">
        <%JSON.stringify(categories) %> <% for (const x in categories) { %>
        <a href="home/<%= categories[x].category %>">
          <%= categories[x].category %>
        </a>
      </br>
     <% } %>
      </form>
    </main>
    <%- include('partials/_footer') %>
  </body>
